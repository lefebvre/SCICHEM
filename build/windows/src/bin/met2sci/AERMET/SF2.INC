C  INCLUDE FILE: SF2.INC
C
      INTEGER      DEV20,DEV21,DEV22,DEV23, SFDCD,SFDAY1,SFDAY2
      INTEGER      SFYR1,SFYR2,SFGDY1,SFGDY2,SFGMO1,SFGMO2
      INTEGER      SFGYR,SFGDY,SFGMO,SFGHR,SFDAYC,SFLDY,SFLYR
      INTEGER      SFLMO,SFLHR,SFDAY,SFMO,SFYR,SFHR,SFQA,SFQA1,SFLST
      INTEGER      SFOVR, SFSAUD, SFAUD, SFAUD1, SFAUD2, SFSTRA
      INTEGER      IASOSYR, IASOSMO, IASOSDAY, IASOSHR
      CHARACTER*1  SFASOS, SFGASOS
      CHARACTER*96 UNIT20, DISK21, DISK22, DISK23
      CHARACTER*8  SFLOC1
      CHARACTER*2  SFBLK
C
      COMMON /SFBK05/ DEV20,DEV21,DEV22, DEV23
      COMMON /SFBK06/ UNIT20, DISK21, DISK22, DISK23, SFBLK,SFLOC1
      COMMON /SFBK07/ SFDCD,SFDAY1,SFDAY2,SFYR1,SFYR2,SFGDY1,
     1                SFGDY2, SFGMO1,SFGMO2,SFGDY,SFGMO,SFGYR,SFGHR,
     1                SFDAYC,SFLDY,SFLMO,SFLYR,SFLHR,SFOVR(35)
      COMMON /SFBK08/ SFDAY(SFMH),SFMO(SFMH),SFYR(SFMH),SFHR(SFMH),
     1                SFQA(SFMV,4),SFQA1(SFMV,2),SFSAUD(22),SFSTRA(22),
     2                SFAUD(22,0:3),SFAUD1(11,0:3),SFAUD2(11,0:3),SFLST
      COMMON /SFBK09/ SFASOS(SFMH),SFGASOS
      COMMON /ASOSBK02/ IASOSYR(ASOSMH), IASOSMO(ASOSMH),
     &                  IASOSDAY(ASOSMH), IASOSHR(ASOSMH)
C
C       DEV20           DEVICE NUMBER FOR HOURLY WXX OBSERVATION DATA
C       DEV21           DEVICE NUMBER FOR SF QA INPUT DATA
C       DEV22           DEVICE NUMBER FOR SF QA OUTPUT DATA
C       DEV23           DEVICE NUMBER FOR 1-MIN ASOS DATA (FOR STAGE2 ONLY)
C       UNIT20          FILENAME FOR DEV20
C       DISK21          FILENAME FOR DEV21
C       DISK22          FILENAME FOR DEV22
C       DISK23          FILENAME FOR DEV23
C       SFDCD           FORMAT CODE OF DATA ON TAPE
C       SFBLK           BLOCKING FORMAT ON THE TAPE ('FB' OR 'VB')
C       SFDAY1,SFDAY2   START, STOP CHRONOLOGICAL DAYS FOR SF EXTRACT
C       SFYR1,SFYR2     START AND STOP YEARS FOR EXTRACT DATA
C       SFGDY1,SFGDY2   START AND STOP DAYS (GREGORIAN)
C       SFGMO1,SFGMO2   START AND STOP MONTHS (GREGORIAN)
C       SFGDY...SFGHR   CURRENT OBSERVATION DATE BEING PROCESSED
C       SFDAYC          CURRENT CHRONOLOGICAL DAY BEING PROCESSED
C       SFLDY...SFLHR   LAST OBSERVATION DATE PROCESSED
C       SFLOC1          STATION ID FOR SURFACE DATA EXTRACTION
C       SFOVR           ARRAY OF OVERPUNCHES FOR SURFACE EXTRACT
C       SFDAY...SFHR    DAY, MONTH, YEAR AND HOUR FOR EACH HOUR MERGED
C       SFQA            LOWER AND UPPER RANGE CHECK BOUNDS FOR WXX
C                       VARIABLES
C       SFQA1(I,J)      LEADING (J=1) AND TRAILING(J=2) MISSING VALUE
C                        INDICATORS FOR CONCATENATED VARIABLES
C       SFSAUD(I)       AUDIT VARIABLE INDICATOR
C       SFSTRA(I)       TRACE VARIABLE INDICATOR
C       SFAUD(I,J)      AUDIT RESULTS  FOR SURFACE DATA (I=VBL #),
C                        EXCLUDING CONCATENATED VARIABLES
C                        J = 0 QA GOOD COUNTER
C                            1 MISSING COUNTER
C                            2 LOWER BOUND VIOLATION COUNTER
C                            3 UPPER BOUND VIOLATION COUNTER
C       SFAUD1(I,J)     AUDIT RESULTS (J=0,3) FOR LEADING CONCATENATED
C                        VARIABLE (AUDIT INDICATOR (J=4) IS IN SFAUD)
C       SFAUD2(I,J)     AUDIT RESULTS (J=0,3) FOR TRAILING CONCATENATED
C                        VARIABLE (AUDIT INDICATOR (J=4) IS IN SFAUD)
C       SFLST           CONVERSION FACTOR FOR USER TIME TO SOLAR TIME,'
C                        SUCH THAT USER TIME - FACTOR = SOLAR TIME
C
